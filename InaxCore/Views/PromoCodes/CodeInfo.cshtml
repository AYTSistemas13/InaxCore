@model  List<RedeemedCodeInfo>

@{
    ViewData["Title"] = "Información de código promocional";
}

    <div class="container-fluid mx-0 my-5 pt-5">
        <div class="row">
            <div class="col-5">
                <h1>@ViewData["Title"] </h1>
            </div>
        </div>
        <table id="search_sales" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Cliente</th>
                    <th>Cotización</th>
                    <th>Fecha de creación</th>
                    @*<th>Acciones</th>*@
                </tr>
            </thead>
            <tbody>
                @foreach (RedeemedCodeInfo promoCode in Model)
                {
                <tr>
                    <td>@promoCode.Id</td>
                    <td>@promoCode.ClientName</td>
                    <td>@promoCode.Cot</td>
                    <td>@promoCode.InsertDate</td>
                    @*<td>POR PROGRAMAR</td>*@
                </tr>
                }
            </tbody>
        </table>
    </div>

